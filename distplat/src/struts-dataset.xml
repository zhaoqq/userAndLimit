<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
        "http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>

    <package name="dataset" namespace="/" extends="struts-default">
        <action name="doDataPut" class="dataAction" method="putData">
            <result name="success">/JSP/dataset/success.jsp</result>
            <result name="error">/JSP/dataset/error.jsp</result>
            <result name="input">/JSP/dataset/error.jsp</result>
        </action>

    </package>
</struts>